    "def giaiThua(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    return n * giaiThua(n - 1)\n",
    " \n",
    "\n",
    "n = int(input(\"Nhập số cần tính giai thừa: \")) \n",
    "print (giaiThua(n))"
   ]
