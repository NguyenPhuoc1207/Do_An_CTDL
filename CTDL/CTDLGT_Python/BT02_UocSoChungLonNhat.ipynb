   "source": [
    "def uscln(a, b):\n",
    "    if (b == 0):\n",
    "        return a\n",
    "    return uscln(b, a % b)\n",
    "\n",
    "a = int(input(\"Số dương thứ nhất: \"))\n",
    "b = int(input(\"Số dương thứ hai: \"))\n",
    "\n",
    "print(\"Ước số chung lớn nhất của\", a, \"và\", b, \"là:\", uscln(a, b))"
   ]
